s,@RANLIB@,$RANLIB,;t t
s,@ac_ct_RANLIB@,$ac_ct_RANLIB,;t t
s,@YACC@,$YACC,;t t
s,@TAR@,$TAR,;t t
s,@ZIP@,$ZIP,;t t
